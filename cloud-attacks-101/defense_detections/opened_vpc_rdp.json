{
    "size": 0,
    "query": {
        "bool": {
            "filter": [
                {
                    "term": {
                        "data.aws.action": {
                            "value": "ACCEPT",
                            "boost": 1
                        }
                    }
                },
                {
                    "term": {
                        "data.aws.dstport": {
                            "value": [3389, 5900],
                            "boost": 1
                        }
                    }
                }
            ],
            "must_not": [
                {
                    "range": {
                        "data.aws.srcaddr": {
                            "from": "10.0.0.0",
                            "to": "10.255.255.255",
                            "include_lower": true,
                            "include_upper": true,
                            "boost": 1
                        }
                    }
                },
                {
                    "range": {
                        "data.aws.srcaddr": {
                            "from": "172.16.0.0",
                            "to": "172.31.255.255",
                            "include_lower": true,
                            "include_upper": true,
                            "boost": 1
                        }
                    }
                },
                {
                    "range": {
                        "data.aws.srcaddr": {
                            "from": "192.168.0.0",
                            "to": "192.168.255.255",
                            "include_lower": true,
                            "include_upper": true,
                            "boost": 1
                        }
                    }
                }
            ],
            "adjust_pure_negative": true,
            "boost": 1
        }
    }
}